{
    "name"       : "rarst/sideface",
    "description": "Web app viewer for XHProfâ€“compatible performance profile files.",
    "type"       : "project",
    "keywords"   : [
        "profiling",
        "performance"
    ],
    "homepage"   : "https://github.com/Rarst/sideface",
    "license"    : "Apache-2.0",
    "authors"    : [
        {
            "name"    : "Andrey Savchenko",
            "homepage": "https://www.Rarst.net/"
        }
    ],
    "support"    : {
        "issues": "https://github.com/Rarst/sideface/issues"
    },
    "require"    : {
        "php"                             : ">=7.1.3",
        "twig/twig"                       : "^2.7.4",
        "components/jquery"               : "^2.1.3",
        "twbs/bootstrap"                  : "^3.3.1",
        "frontpack/composer-assets-plugin": "^0.9.4",
        "slim/slim"                       : "^3.12",
        "slim/twig-view"                  : "^2.5"
    },
    "require-dev": {
        "pds/skeleton": "^1.0"
    },
    "autoload"   : {
        "classmap": [
            "src/legacy/"
        ],
        "psr-4"   : {
            "Rarst\\Sideface\\": "src/"
        },
        "files"   : [
            "src/legacy/functions.php"
        ]
    },
    "config"     : {
        "assets-files": {
            "components/jquery": [
                "jquery.js",
                "jquery.min.js",
                "jquery.min.map"
            ],
            "twbs/bootstrap"   : [
                "dist/css",
                "dist/fonts",
                "dist/js"
            ]
        },
        "assets-target": {
            "components/jquery": "public/assets/jquery-2",
            "twbs/bootstrap"   : "public/assets/bootstrap"
        }
    }
}
