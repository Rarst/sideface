{% extends 'index.twig' %}

{% block body %}
    {{ block('breadcrumb') }}
    <table class="table table-condensed">
        <tr>
            <th>Source</th>
            <th>ID</th>
            <th>Date</th>
            <th>Type</th>
        </tr>
        {% for run in runs %}
            <tr>
                <td>{{ run.source }}</td>
                <td>{{ run.id }}</td>
                <td>{{ run.time }}</td>
                <td>{{ run.suffix }}</td>
            </tr>
        {% endfor %}
    </table>
{% endblock %}